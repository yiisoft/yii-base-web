{
    "name": "yiisoft/app",
    "description": "Yii 3.0 project template",
    "keywords": ["yii2", "framework", "project template"],
    "homepage": "https://github.com/yiisoft/yii2-app",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/github.com/yiisoft/yii2-app/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "source": "https://github.com/yiisoft/yii2-app"
    },
    "authors": [
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "hiqdev/composer-config-plugin": "3.0.x-dev",
        "hiqsol/web": "dev-master",
        "hiqsol/log": "dev-master",
        "yiisoft/di": "dev-master",
        "yiisoft/yii2-jquery": "1.0.x-dev",
        "yiisoft/yii2-captcha": "1.0.x-dev",
        "yiisoft/yii2-bootstrap": "2.2.x-dev",
        "yiisoft/yii2-swiftmailer": "2.2.x-dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "2.1.x-dev",
        "yiisoft/yii2-gii": "2.1.x-dev",
        "phpunit/phpunit": "~7.0.2"
    },
    "extra": {
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "web": "config/web.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"yii\\app\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yii\\tests\\": "tests"}
    },
    "config": {
        "process-timeout": 1800
    }
}
